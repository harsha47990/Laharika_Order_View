<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<appSettings>
		<add key="OrderDetailsPath" value="D:\others\test environment\Server\Print Order Details"/>
		<add key="Filter" value="$_Copied"/>   <!-- get filename without extension using Contains Cond and for reading files as well (.txt is defined inside code)-->
